<!DOCTYPE html>
<html>
    <head>
        <meta charset='utf-8'>
        <title>Hello!</title>
    </head>
    <body>
        @{
            List<string> myList = new List<string>
                {
                    "One",
                    "Two",
                    "Three",
                    "Four",
                    "Not Five"
                };
            foreach(string word in myList)
            {
                <p>@word</p>
            }
        }
        <h1>Hello ASP.NET Mvc!</h1>
    </body>
</html>