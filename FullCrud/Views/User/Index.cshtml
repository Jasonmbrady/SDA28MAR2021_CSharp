@model User;

<form asp-action="Create" asp-controller="User" method="post">
    <div><span asp-validation-for="FirstName"></span></div>
    <label asp-for="FirstName"></label>
    <input asp-for="FirstName">
    <input value="Create User" type="Submit">
</form>
    <ul>
@{
    foreach(User u in ViewBag.AllUnicorns)
    {
        <li><a href="/@u.UserId">@u.FirstName</a> <a href="/delete/@u.UserId"><button>DELETE</button></a></li>
    }
}
    </ul>